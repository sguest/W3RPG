    call SaveInteger(udg_StatModTable, armorHash, 128, 'A01W')
    call SaveInteger(udg_StatModTable, armorHash, -256, 'A01P')

    call SaveInteger(udg_StatModTable, maxValueHash, healthRegenHash, 64)
    call SaveInteger(udg_StatModTable, healthRegenHash, 1, 'A025')
    call SaveInteger(udg_StatModTable, healthRegenHash, 2, 'A01Y')
    call SaveInteger(udg_StatModTable, healthRegenHash, 4, 'A01Z')
    call SaveInteger(udg_StatModTable, healthRegenHash, 8, 'A020')
    call SaveInteger(udg_StatModTable, healthRegenHash, 16, 'A021')
    call SaveInteger(udg_StatModTable, healthRegenHash, 32, 'A022')
    call SaveInteger(udg_StatModTable, healthRegenHash, 64, 'A023')
    call SaveInteger(udg_StatModTable, healthRegenHash, -128, 'A024')

    call SaveInteger(udg_StatModTable, maxValueHash, attackSpeedHash, 128)
    call SaveInteger(udg_StatModTable, healthRegenHash, 1, 'A02A')
    call SaveInteger(udg_StatModTable, healthRegenHash, 2, 'A02C')
    call SaveInteger(udg_StatModTable, healthRegenHash, 4, 'A027')
    call SaveInteger(udg_StatModTable, healthRegenHash, 8, 'A028')
    call SaveInteger(udg_StatModTable, healthRegenHash, 16, 'A02B')
    call SaveInteger(udg_StatModTable, healthRegenHash, 32, 'A026')
    call SaveInteger(udg_StatModTable, healthRegenHash, 64, 'A02D')
    call SaveInteger(udg_StatModTable, healthRegenHash, 128, 'A01A')
    call SaveInteger(udg_StatModTable, healthRegenHash, -256, 'A029')
endfunction