function InitStatMod takes nothing returns nothing
    local integer attackHash = StringHash("attack")
    local integer healthHash = StringHash("health")
    local integer armorHash = StringHash("armor")
    local integer healthRegenHash = StringHash("healthRegen")
    local integer attackSpeedHash = StringHash("attackSpeed")
    local integer maxValueHash = StringHash("MaxValue")
    set udg_StatModTable = InitHashtable()

    call SaveInteger(udg_StatModTable, maxValueHash, attackHash, 256)
    call SaveInteger(udg_StatModTable, attackHash, 1, 'A00Z')
    call SaveInteger(udg_StatModTable, attackHash, 2, 'A010')
    call SaveInteger(udg_StatModTable, attackHash, 4, 'A011')
    call SaveInteger(udg_StatModTable, attackHash, 8, 'A012')
    call SaveInteger(udg_StatModTable, attackHash, 16, 'A013')
    call SaveInteger(udg_StatModTable, attackHash, 32, 'A014')
    call SaveInteger(udg_StatModTable, attackHash, 64, 'A015')
    call SaveInteger(udg_StatModTable, attackHash, 128, 'A016')
    call SaveInteger(udg_StatModTable, attackHash, 256, 'A017')
    call SaveInteger(udg_StatModTable, attackHash, -512, 'A018')

    call SaveInteger(udg_StatModTable, maxValueHash, healthHash, 4096)
    call SaveInteger(udg_StatModTable, healthHash, 1, 'A01C')
    call SaveInteger(udg_StatModTable, healthHash, 2, 'A01B')
    call SaveInteger(udg_StatModTable, healthHash, 4, 'A01O')
    call SaveInteger(udg_StatModTable, healthHash, 8, 'A01D')
    call SaveInteger(udg_StatModTable, healthHash, 16, 'A01E')
    call SaveInteger(udg_StatModTable, healthHash, 32, 'A01F')
    call SaveInteger(udg_StatModTable, healthHash, 64, 'A01G')
    call SaveInteger(udg_StatModTable, healthHash, 128, 'A01H')
    call SaveInteger(udg_StatModTable, healthHash, 256, 'A01I')
    call SaveInteger(udg_StatModTable, healthHash, 512, 'A01J')
    call SaveInteger(udg_StatModTable, healthHash, 1024, 'A01K')
    call SaveInteger(udg_StatModTable, healthHash, 2048, 'A01L')
    call SaveInteger(udg_StatModTable, healthHash, 4096, 'A01M')
    call SaveInteger(udg_StatModTable, healthHash, -8192, 'A01N')

    call SaveInteger(udg_StatModTable, maxValueHash, armorHash, 128)
    call SaveInteger(udg_StatModTable, armorHash, 1, 'A01X')
    call SaveInteger(udg_StatModTable, armorHash, 2, 'A01Q')
    call SaveInteger(udg_StatModTable, armorHash, 4, 'A01R')
    call SaveInteger(udg_StatModTable, armorHash, 8, 'A01S')
