import "/heroes/farSeer/feralSpirit.jass"
import "/heroes/farSeer/chainLightning.jass"
import "/heroes/farSeer/earthquake.jass"
import "/heroes/farSeer/lightningShield.jass"
import "/heroes/farSeer/earthShield.jass"

function InitFarSeer takes unit hero returns nothing
    call InitFeralSpirit(hero)
    call InitChainLightning(hero)
    call InitEarthquake(hero)
    call InitLightningShield(hero)
    call InitEarthShield(hero)
endfunction