import "/heroes/demonHunter/manaBurn.jass"
import "/heroes/demonHunter/immolation.jass"
import "/heroes/demonHunter/metamorphosis.jass"

function InitDemonHunter takes unit hero returns nothing
    call InitLevelStats('Edem', 60, 0.12, 3, 0.45, 31.5, 10.5)
    call InitManaBurn(hero)
    call InitImmolation(hero)
    call InitMetamorphosis(hero)
endfunction