import "/statMod/init.jass"
import "/heroes/init.jass"
import "/shops/main.jass"

function InitGlobalTables takes nothing returns nothing
    set udg_ChannelCasters = InitHashtable()
    set udg_ShieldTable = InitHashtable()
    set udg_AbilityToggleTable = InitHashtable()
    set udg_HeroLevelStats = InitHashtable()
endfunction

function InitMain takes nothing returns nothing
    call InitGlobalTables()
    call InitStatMod()
    call InitHeroSelection()
    call InitShops()
endfunction